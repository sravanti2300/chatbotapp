{
    "rules": {
      "no-underscore-dangle": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
      "no-use-before-define": ["error", { "variables": false }],
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "extends": [
      "airbnb-base"
    ]
  }
  